<!DOCTYPE html>

<head>
    <meta charset="UTF-8">
    <title>{% block title %}{{title}}{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block stylesheets %}
    {{ encore_entry_link_tags('bulma_variables') }}
    {{ encore_entry_link_tags('app_scss') }}
    {{ encore_entry_link_tags('bulma') }}
    {% endblock %}
</head>

<body>
    {% include 'includes/navbar.html.twig' %}
    <section class='container'>
        {% block body %}
        {% endblock %}
    </section>
    {% block javascripts %}

    {{ encore_entry_script_tags('app_js') }}
    <script src="https://kit.fontawesome.com/18f650d607.js" crossorigin="anonymous"></script>
    {% endblock %}
</body>

</html>