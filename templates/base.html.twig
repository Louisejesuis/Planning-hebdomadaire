<!DOCTYPE html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block stylesheets %}
    {{ encore_entry_link_tags('bulma') }}
    {{ encore_entry_link_tags('bulma_calendar_css') }}
    {{ encore_entry_link_tags('app_scss') }}
    {% endblock %}
    </head>
    <body>
        {% include 'includes/navbar.html.twig' %}
        <div class='content'>
            {% block body %}
            {% endblock %}
        </div>
        {% block javascripts %}
        {{ encore_entry_script_tags('app_js') }}
        {{ encore_entry_script_tags('bulma_calendar_js') }}
        {% endblock %}
    </body>
</html>
